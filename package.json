{
  "name": "chat",
  "version": "0.1.0",
  "hommepage": "https://zhoubichuan.github.io/wechat-vue/",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generateAnalyzFile": "webpack --profile --json > stats.json",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true npm run build",
    "deploy": "bash deploy.sh",
    "gh-pages": "bash deploy_gh-pages.sh"
  },
  "dependencies": {
    "axios": "0.18.0",
    "babel": "6.23.0",
    "babel-loader": "8.0.5",
    "cache-loader": "2.0.1",
    "core-js": "2.6.5",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "vue": "^2.7.16",
    "vue-i18n": "8.17.2",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "7.24.7",
    "@vue/cli-plugin-babel": "3.12.1",
    "@vue/cli-plugin-eslint": "3.12.1",
    "@vue/cli-service": "3.12.1",
    "autoprefixer": "9.7.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "cross-env": "6.0.3",
    "eslint": "5.16.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-vue": "5.0.0",
    "gh-pages": "5.0.0",
    "html-withimg-loader": "0.1.16",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "vue-template-compiler": "2.7.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
